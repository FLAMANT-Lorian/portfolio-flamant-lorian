{
  "name": "portfolio-flamant-lorian",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "chokidar './wp-content/themes/portfolio/resources/js/app/**/*.js' './wp-content/themes/portfolio/resources/css/**/*' -c 'vite build'",
    "serve": "browser-sync start --proxy 'http://portfolio-flamant-lorian.test/' --files 'wp-content/themes/portfolio/public/**/*' --no-https --port 8000"
  },
  "author": "Lorian Flamant",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "chokidar-cli": "^3.0.0",
    "glob": "^11.0.2",
    "postcss": "^8.5.3",
    "sass": "^1.87.0",
    "vite": "^6.3.4"
  },
  "browserslist": [
    "last 2 versions",
    "iOS >= 10",
    "IE 11",
    "not IE 10",
    "Firefox ESR",
    "Chrome >= 50",
    "Android >= 4.4",
    "Safari >= 9"
  ]
